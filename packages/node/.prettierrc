{
	"printWidth": 60,
	"tabWidth": 4,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"trailingComma": "all",
	"arrowParens": "avoid",
	"proseWrap": "always",
	"htmlWhitespaceSensitivity": "ignore",
	"endOfLine": "lf",
	"overrides": [
		{
			"files": ["*.yaml", "*.yml"],
			"options": {
				"tabWidth": 2
			}
		},
		{
			"files": ".swcrc",
			"options": {
				"parser": "json",
				"tabWidth": 4
			}
		}
	]
}
