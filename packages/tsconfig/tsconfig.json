{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"composite": false,
		"declaration": true,
		"declarationMap": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"incremental": true,
		"inlineSources": false,
		"isolatedModules": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"preserveWatchOutput": true,
		"skipDefaultLibCheck": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "es2022"
	}
}
